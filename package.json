    "galenframework": "^2.3.3",
    "gulp": "^3.9.1",
    "gulp-galen": "^1.1.1",
    "gulp-watch": "^4.3.11",
    "html-webpack-plugin": "^2.28.0",
    "image-webpack-loader": "^3.4.2",
    "path": "^0.12.7",
    "postcss": "^5.2.15",
    "postcss-cssnext": "^2.9.0",
    "postcss-import": "^9.1.0",
    "postcss-loader": "^1.3.3",
    "transfer-webpack-plugin": "^0.1.4",
    "webpack": "3.1.0",
