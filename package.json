  "scripts": {
    "build": "npm run clear && npm i && npm run prod",
    "prod": "rm -rf dist && NODE_ENV='production' webpack -p",
    "start:dev": "webpack-dev-server",
    "start:galen": "gulp watch",
    "clear": "rm -rf node_modules && rm -rf dist",
    "test:galen": "galen test galen_test/prod/start_test.test --htmlreport reports --parallel-tests 6",
    "report:open": "open galen_test/reports/report.html",
    "report:clear": "rm -rf galen_test/reports"
  },
  "devDependencies": {
    "babel-core": "^6.23.1",
